// void function();
String command;
void FUNC2()
{
  Serial.print("\nFunc2 is called\n");
}
void function()
{ 
  Serial.print("\nFunction is called. \n");
  
  
  return 0;
}
void setup()
{
  Serial.begin(9600);
  // function(command);
}


void loop() 
{
  if(Serial.available()){
    command = Serial.readStringUntil('\n');
    if(command.equals("a")){
      Serial.print("\nIT WORKS\n");
      function();
      
    }
    else
    {
      FUNC2();
    }
    
  }

}

